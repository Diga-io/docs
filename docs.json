{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Diga",
  "colors": {
    "primary": "#343E96",
    "light": "#D7DAFF",
    "dark": "#343E96"
  },
  "background": {
    "color": {
      "light": "#FDFDFD",
      "dark": "#010318"
    }
  },
  "favicon": {
    "light": "/logo/fav-light.svg",
    "dark": "/logo/fav-dark.svg"
  },
  "fonts": {
    "heading": {
      "family": "Geist",
      "weight": 700
    },
    "body": {
      "family": "Sora",
      "weight": 400
    }
  },
  "icons": {
    "library": "lucide"
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://mintlify.com/docs",
          "icon": "discord"
        },
        {
          "anchor": "Changelog",
          "href": "https://mintlify.com/blog",
          "icon": "clock"
        },
        {
          "anchor": "Pricing",
          "icon": "dollar-sign",
          "href": "https://mintlify.com/pricing"
        }
      ]
    },
    "languages": [
      {
        "language": "en",
        "tabs": [
          {
            "tab": "Documentation",
            "groups": [
              {
                "group": "Get Started",
                "pages": [
                  "en/index",
                  "en/quickstart",
                  "en/development"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "openapi": "/api-reference/openapi.json"
          }
        ]
      },
      {
        "language": "es",
        "default": true,
        "tabs": [
          {
            "tab": "Documentación",
            "groups": [
              {
                "group": "Comenzar",
                "pages": [
                  "es/index",
                  "es/crear-agente"
                ]
              },
              {
                "group": "Construir",
                "pages": [
                  {
                    "group": "Agentes",
                    "pages": [
                      "es/build/agentes/index",
                      "es/build/agentes/prompt-unico",
                      "es/build/agentes/camino-conversacional",
                      "es/build/agentes/personalizacion-llamada",
                      "es/build/agentes/control-versiones",
                      "es/build/agentes/herramientas-llamada"
                    ]
                  },
                  {
                    "group": "Integraciones",
                    "pages": [
                      "es/build/herramientas/introduccion",
                      "es/build/herramientas/integraciones-MCP",
                      "es/build/herramientas/integraciones-HTTP"
                    ]
                  }
                ]
              },
              {
                "group": "Telefonía",
                "pages": [
                  "es/despliegue/comprar-telefonos",
                  "es/despliegue/sip-trunks"
                ]
              },
              {
                "group": "Cuentas y proyectos",
                "pages": [
                  "es/proyectos/cuentas",
                  "es/proyectos/proyectos"
                ]
              },
              {
                "group": "Analitícas",
                "pages": [
                  "es/despliegue/monitoreo-logs"
                ]
              },
              {
                "group": "Guías avanzadas",
                "pages": [
                ]
              }
            ]
          },
          {
            "tab": "API reference",
            "openapi": "/api-reference/openapi.json"
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["python", "javascript", "curl", "go"],
      "defaults": "all"
    }
  },
  "logo": {
    "light": "/logo/primary_log.svg",
    "dark": "/logo/white_logo.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
