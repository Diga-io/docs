{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Diga",
  "colors": {
    "primary": "#343E96",
    "light": "#D7DAFF",
    "dark": "#343E96"
  },
  "background": {
    "color": {
      "light": "#FDFDFD",
      "dark": "#010318"
    }
  },
  "favicon": {
    "light": "/logo/fav-light.svg",
    "dark": "/logo/fav-dark.svg"
  },
  "fonts": {
    "heading": {
      "family": "Sora",
      "weight": 700
    },
    "body": {
      "family": "Geist",
      "weight": 500
    }
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://mintlify.com/docs",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    },
    "languages": [
      {
        "language": "en",
        "tabs": [
          {
            "tab": "Documentation",
            "groups": [
              {
                "group": "Get Started",
                "pages": [
                  "en/index",
                  "en/quickstart",
                  "en/development"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "openapi": "/api-reference/openapi.json"
          }
        ]
      },
      {
        "language": "es",
        "default": true,
        "tabs": [
          {
            "tab": "Documentaci√≥n",
            "groups": [
              {
                "group": "Comenzar",
                "pages": [
                  "es/index",
                  "es/quickstart",
                  "es/development"
                ]
              }
            ]
          },
          {
            "tab": "API reference",
            "openapi": "/api-reference/openapi.json"
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["python", "javascript", "curl", "go"],
      "defaults": "all"
    }
  },
  "logo": {
    "light": "/logo/primary_log.svg",
    "dark": "/logo/white_logo.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
